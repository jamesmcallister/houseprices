{
  "name": "houseprices",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "format": "prettier-eslint --write \"app/**/*.js\"",
    "yarn": "yarn",
    "clean": "rimraf app/target",
    "shell": "docker exec -ti influxdb bash",
    "start": "docker-compose up --build --remove-orphans",
    "start:d": "docker-compose up --build -d --remove-orphans",
    "stop": "docker-compose stop",
    "deploy": "ansible-playbook ./ansible/playbook.yml -i do-droplet-house-prices,",
    "compile": "npm run clean && babel ./app/main/src --out-dir ./app/target/src",
    "test-compile": "babel ./app/main/test --out-dir ./app/target/test",
    "int-compile": "babel ./app/main/integration --out-dir ./app/target/integration",
    "ingest": "npm run compile && node ./app/target/src/ingest.js",
    "int": "export INFLUXDB_HOST=\"$(docker inspect --format '{{ .NetworkSettings.IPAddress }}' influxdb)\" && npm run compile && npm run int-compile && mocha ./app/target/integration/**/*",
    "influxtest": "export INFLUXDB_HOST=\"$(docker inspect --format '{{ .NetworkSettings.IPAddress }}' influxdb)\" && npm run compile && node ./app/target/src/influxtest.js",
    "test": "export INFLUXDB_HOST=\"$(docker inspect --format '{{ .NetworkSettings.IPAddress }}' influxdb)\" && jest",
    "jest": "export INFLUXDB_HOST=\"$(docker inspect --format '{{ .NetworkSettings.IPAddress }}' influxdb)\" && jest",
    "test:watch": "export INFLUXDB_HOST=\"$(docker inspect --format '{{ .NetworkSettings.IPAddress }}' influxdb)\" && jest --watch"
  },
  "author": "",
  "license": "UNLINCED",
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "csvtojson": "1.1.5",
    "eslint": "3.19.0",
    "eslint-plugin-prettier": "2.0.1",
    "influx": "5.0.7",
    "jest": "^20.0.3",
    "mocha": "3.3.0",
    "prettier": "1.3.1",
    "prettier-eslint": "6.2.0",
    "prettier-eslint-cli": "3.4.3",
    "rimraf": "2.6.1",
    "sinon": "2.2.0",
    "watch-run": "1.2.5",
    "winston": "2.3.1",
    "yarn": "0.23.4"
  }
}
